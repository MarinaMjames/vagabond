<div class="uk-container" id="loginBox">
	<fieldset class="uk-fieldset">
		<form>
			<legend class="uk-legend">Login</legend>
			<div class="uk-margin">
				<input class="uk-input uk-form-width-medium" type="text" placeholder="User Name">
			</div>
			<div class="uk-margin">
				<input class="uk-input uk-form-width-medium" type="text" placeholder="Password">
			</div>
		</form>
		<p id="accountYetText">Dont Have an Account Yet?</p>
		<br>
		<button class="uk-button uk-button-primary" id="newUserButton">SIGN UP</button>
	</fieldset>
</div>
