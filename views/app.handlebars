<center>

<div class="background uk-width-1-1" id="containerBox">

<form action="/city/:city" method="GET" id="cityDropDown">

    <select class="uk-select uk-form-width-medium">
        <option name="city" value="atlanta">Atlanta</option>
        <option name="city" value="austin">Austin</option>
        <option name="city" value="boston">Boston</option>
        <option name="city" value="chicago">Chicago</option>
        <option name="city" value="cleveland">Cleveland</option>
        <option name="city" value="denver">Denver</option>
        <option name="city" value="las vegas">Las Vegas</option>
        <option name="city" value="los angeles">Los Angeles</option>
        <option name="city" value="nashville">Nashville</option>
        <option name="city" value="new orleans">New Orleans</option>
        <option name="city" value="new york city">New York City</option>
        <option name="city" value="philadelphia">Philadelphia</option>
        <option name="city" value="portland">Portland</option>
        <option name="city" value="san francisco">San Francisco</option>
        <option name="city" value="seattle">Seattle</option>
        <option name="city" value="washington dc">Washington D.C.</option>
    </select>

    <button class="uk-button uk-button-default" type="submit">Search

    </button>
</form>



<div id="city-info">
   {{#each City}}
      <p>{{bio}}. {{this}}</p>
   {{/each}}
</div>


<div id="carousel-example-generic" class="carousel slide slideshow-container" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>
 <!-- Wrapper for slides -->
<div class="carousel-inner" role="listbox">
    <div class="item active">
      <img src="" id ="Image1" alt="...">
    </div>
<!-- Slide 2 -->
   <div class="item">
      <img src="" id ="Image2" alt="...">
    </div>
<!-- Slide 3 -->
   <div class="item">
      <img src="" id ="Image3" alt="...">
    </div>
<!-- Slide 4 -->
   <div class="item">
      <img src="" id ="Image4" alt="...">
    </div>
<!-- Slide 5 -->
   <div class="item">
      <img src="" id ="Image5" alt="...">
    </div>
<!-- Slide 6 -->
   <div class="item">
      <img src="" id ="Image6" alt="...">
    </div>
<!-- Slide 7 -->
   <div class="item">
      <img src="" id ="Image7" alt="...">
    </div>
<!-- Slide 8 -->
   <div class="item">
      <img src="" id ="Image8" alt="...">

    </div>



  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
</div>

<div class="uk-container uk-width-3-4" id="attractionBox">
<h3>Attractions: </h3>
{{#each city}}
    <p>{{this.landmark1}}</p>
    <p>{{this.landmark2}}</p>
    <p>{{this.landmark3}}</p>
    <p>{{this.landmark4}}</p>
    <p>{{this.landmark5}}</p>
    <p>{{this.landmark6}}</p>
    <p>{{this.landmark7}}</p>
    <p>{{this.landmark8}}</p>
    <p>{{this.landmark9}}</p>
    <p>{{this.landmark10}}</p>
{{/each}}
</div>

</center>

<script type="text/javascript" src="/js/pixabay.js"></script>