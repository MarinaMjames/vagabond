<!-- Navbar -->
<nav class="uk-navbar-container" uk-navbar>
  <div class="uk-navbar-item uk-logo vagabondLogo uk-navbar-left">Vagabond</div>
  <div class="uk-navbar-item uk-navbar-right">
    <a class="uk-button uk-button-default" type="button" href="/" uk-icon="icon: home">
    </a>
    <a class="uk-button uk-button-default" type="button" href="/wishlist" uk-icon="icon: heart">Wishlist
    </a>
    <a class="uk-button uk-button-default" type="button" href="/account" uk-icon="icon: user">
    </a>
